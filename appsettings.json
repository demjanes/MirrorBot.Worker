{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information",
      "UpdateHandler": "Debug", // debug только для хендлера
      "PollingService": "Information"
    }
  },
  "BotConfiguration": {
    "BotToken": "YOUR_BOT_TOKEN",
    //[Convert]::ToBase64String((1..32 | ForEach-Object { [byte](Get-Random -Maximum 256) })) - генерация через PS
    "EncryptionKey": "your_super_secret_key_at_least_32_characters_long"
  },
  "Limits": {
    "MaxBotsPerUser": 3
  },
  "Mongo": {
    "ConnectionString": "BASE_CONNECTION_STRING",
    "Database": "DATABASE_HAME"
  },
  "AdminNotifications": {
    "Enabled": true,

    "Info": {
      "Enabled": true,
      "ChatId": -1006464646
    },
    "Ref": {
      "Enabled": true,
      "ChatId": -1004546546
    },

    "BatchSize": 1,
    "FlushIntervalMs": 1000,
    "CombineIntoSingleMessage": true,
    "MaxQueueSize": 10000
  }
}
